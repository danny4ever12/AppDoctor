<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appdoctor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.appdoctor.AppDoctor"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.appdoctor.Symptoms"
            android:label="@string/title_activity_symptoms" >
        </activity>
        <activity
            android:name="com.example.appdoctor.Diseases"
            android:label="@string/title_activity_diseases" >
        </activity>
        <activity
            android:name="com.example.appdoctor.Medicines"
            android:label="@string/title_activity_medicines" >
        </activity>
        <activity
            android:name="com.example.appdoctor.HealthNews"
            android:label="@string/title_activity_health_news" >
        </activity>
        <activity
            android:name="com.example.appdoctor.AboutMe"
            android:label="@string/title_activity_about_me" >
        </activity>
        <activity
            android:name="com.example.appdoctor.SubPainkiller"
            android:label="@string/title_activity_sub_painkiller" >
        </activity>
        <activity
            android:name="com.example.appdoctor.SubAntibiotic"
            android:label="@string/title_activity_sub_antibiotic" >
        </activity>
        <activity
            android:name="com.example.appdoctor.SubAntihistamine"
            android:label="@string/title_activity_sub_antihistamine" >
        </activity>
        <activity
            android:name="com.example.appdoctor.InspirationalQuotes"
            android:label="@string/title_activity_inspirational_quotes" >
        </activity>
        <activity
            android:name="com.example.appdoctor.AccountNo"
            android:label="@string/title_activity_account_no" >
        </activity>
        <activity
            android:name="com.example.appdoctor.ResultEvaluate"
            android:label="@string/title_activity_result_evaluate" >
        </activity>
        <activity
            android:name="com.example.appdoctor.DiseaseExplanation"
            android:label="@string/title_activity_disease_explanation" >
        </activity>
        <activity
            android:name="com.example.appdoctor.SubAntiinflamatory"
            android:label="@string/title_activity_sub_antiinflamatory" >
        </activity>
        <activity android:name="com.example.appdoctor.Notify" >
        </activity>
        <activity android:name="com.example.appdoctor.AlarmListActivity" >
        </activity>
        <activity android:name="com.example.appdoctor.AlarmScreen" >
        </activity>
        <activity android:name="com.example.appdoctor.AlarmDetailsActivity" >
        </activity>
        <activity
            android:name="com.example.appdoctor.SetNotification"
            android:label="@string/title_activity_set_notification" >
        </activity>

        <service
            android:name="com.example.appdoctor.AlarmService"
            android:enabled="true" >
        </service>

        <receiver android:name="com.example.appdoctor.AlarmManagerHelper" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
    </application>

</manifest>
